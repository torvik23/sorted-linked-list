{
    "name": "torvik23/sorted-linked-list",
    "type": "library",
    "description": "Sorted linked list data structure for PHP",
    "keywords": [
        "php-data-structures",
        "sorted-linked-list",
        "linked-list"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Viktor Tokar",
            "email": "dev.viktor.tokar@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^11.5",
        "squizlabs/php_codesniffer": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Torvik23\\SortedLinkedList\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Torvik23\\SortedLinkedList\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --no-coverage",
        "phpstan": "phpstan analyse",
        "phpcs": "phpcs",
        "phpcbf": "phpcbf",
        "quality": [
            "@phpcs",
            "@phpstan",
            "@test"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
